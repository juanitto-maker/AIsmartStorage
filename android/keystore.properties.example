# Release keystore configuration
# Copy this file to keystore.properties and fill in your values
# DO NOT commit keystore.properties to version control

storeFile=release-key.keystore
storePassword=your_store_password
keyAlias=your_key_alias
keyPassword=your_key_password

# To generate a release keystore, run:
# keytool -genkey -v -keystore release-key.keystore -alias your_key_alias -keyalg RSA -keysize 2048 -validity 10000
